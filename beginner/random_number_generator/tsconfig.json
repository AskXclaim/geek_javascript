    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ES2020",
    "outDir": "./public",
    "rootDir": "./src",
    "moduleResolution": "node"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
