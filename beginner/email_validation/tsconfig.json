{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "./public/",
    "rootDir": "./src"
  },
  "include": ["src"],

  "exclude": ["node_modules"]
}